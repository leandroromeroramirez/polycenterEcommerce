{
    "name": "polycenter/siigo",
    "description": "SIIGO API integration package for Bagisto",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Polycenter",
            "email": "leandroromeroramirez@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/validation": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Polycenter\\SIIGO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polycenter\\SIIGO\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Polycenter\\SIIGO\\Providers\\SIIGOServiceProvider"
            ],
            "aliases": {
                "SIIGO": "Polycenter\\SIIGO\\Facades\\SIIGO"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
